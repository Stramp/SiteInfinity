{
  "name": "infinity-dragons",
  "version": "0.1.6",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src  --max-warnings=0",
    "tscheck": "tsc --project tsconfig.json --noEmit",
    "generate": "yarn plop --plopfile generators/plopfile.js",
    "test": "jest --maxWorkers=50%",
    "test:wt": "jest --watch --maxWorkers=25%",
    "test:ci": "jest --runInBand",
    "storybook": "set NODE_OPTIONS=--openssl-legacy-provider & start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook -s ./public",
    "postinstall": "husky install"
  },
  "lint-staged": {
    "src/**/*": [
      "yarn lint --fix",
      "yarn test --findRelatedTests --bail --passWithNoTests"
    ]
  },
  "dependencies": {
    "next": "13.1.6",
    "next-pwa": "^5.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",

    "react-icons": "^4.12.0",
    "styled-components": "^5.3.10"

  },
  "devDependencies": {

    "@babel/core": "^7.21.0",
    "@babel/preset-typescript": "^7.18.6",

    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/react": "^6.2.9",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^20.2.3",
    "@types/react": "^18.0.28",
    "@types/styled-components": "^5.1.26",

    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",

    "babel-loader": "^9.1.2",
    "babel-plugin-styled-components": "^2.0.7",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gsap": "^3.11.4",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",

    "jest-styled-components": "^7.1.1",
    "lint-staged": "^13.1.2",
    "plop": "^3.1.2",
    "prettier": "2.8.4",
    "storybook-addon-next-router": "^4.0.2",
    "typescript": "^4.9.5"
  }
}
